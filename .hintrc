{
    "extends": [
        "development"
    ],
    "hints": {
        "no-inline-styles": "off",
        "compat-api/html": [
            "default",
            {
                "ignore": [
                    "meta[name=theme-color]"
                ]
            }
        ],
        "axe/forms": [
            "default",
            {
                "label": "off"
            }
        ],
        "axe/parsing": [
            "default",
            {
                "duplicate-id-active": "off"
            }
        ],
        "compat-api/css": [
            "default",
            {
                "ignore": [
                    "-webkit-tap-highlight-color",
                    "backdrop-filter"
                ]
            }
        ],
        "axe/structure": [
            "default",
            {
                "listitem": "off",
                "list": "off"
            }
        ]
    },
    "browserslist": [
        "defaults",
        "not ie 11",
        "not ios_saf <= 17.1",
        "not safari <= 17.2"
    ]
}